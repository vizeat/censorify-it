    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "build": "rimraf dist && BABEL_ENV=production babel src/ --out-dir dist/ --ignore '**/*.test.js','**/__snapshots__/**' --source-maps --verbose",
